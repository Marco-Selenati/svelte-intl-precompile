{
  "name": "@gigahatch/svelte-intl-precompile",
  "version": "2.0.0",
  "description": "I18n library for Svelte.js that analyzes your keys at build time for max performance and minimal footprint",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./plugin": {
      "import": "./dist/plugin.js",
      "types": "./dist/plugin.d.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gigahatch/svelte-intl-precompile"
  },
  "scripts": {
    "build": "vite build && tsc --project tsconfig.json",
    "postbuild": "cp src/locales.d.ts dist/locales.d.ts",
    "test": "cross-env TZ=utc vitest",
    "coverage": "vitest run --coverage",
    "prepublish": "npm run build"
  },
  "keywords": [
    "svelte",
    "intl",
    "i18n",
    "precompile"
  ],
  "author": "Gigahatch",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.20.5",
    "@babel/helper-plugin-utils": "^7.20.2",
    "@formatjs/icu-messageformat-parser": "^2.1.14",
    "flat": "^5.0.2",
    "hyphen": "^1.6.4"
  },
  "devDependencies": {
    "@babel/types": "^7.20.5",
    "@types/babel__core": "^7.1.20",
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@types/flat": "^5.0.2",
    "@types/hyphen": "^1.6.5",
    "@types/node": "^18.11.17",
    "cross-env": "^7.0.3",
    "typescript": "^4.9.4",
    "vitest": "^0.26.0",
    "vite": "^4.0.2",
    "vite-plugin-dts": "^1.7.1"
  },
  "peerDependencies": {
    "svelte": "^3.53.1"
  }
}

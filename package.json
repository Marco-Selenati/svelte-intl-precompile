{
  "name": "@gigahatch/svelte-intl-precompile",
  "version": "1.0.2",
  "description": "I18n library for Svelte.js that analyzes your keys at build time for max performance and minimal footprint",
  "type": "module",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/plugin": {
      "import": "./dist/plugin.js",
      "types": "./dist/plugin.d.ts"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/gigahatch/svelte-intl-precompile"
  },
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "postbuild": "cp src/locales.d.ts dist/locales.d.ts",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublish": "npm run build"
  },
  "keywords": [
    "svelte",
    "intl",
    "i18n",
    "precompile"
  ],
  "author": "Gigahatch",
  "license": "ISC",
  "dependencies": {
    "@gigahatch/babel-plugin-precompile-intl": "^1.0.0",
    "path-starts-with": "^2.0.0",
    "@gigahatch/precompile-intl-runtime": "^1.0.1"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.19",
    "@types/hyphen": "^1.6.5",
    "@types/node": "^18.11.7",
    "hyphen": "^1.6.4",
    "typescript": "^4.8.4",
    "vitest": "^0.24.3"
  }
}
